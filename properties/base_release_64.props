<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ImportGroup Label="PropertySheets" />
  <PropertyGroup Label="UserMacros">
    <ROOT_DIR>D:\gtkmm\output\$(Configuration)\$(Platform)\</ROOT_DIR>
    <BIN_DIR>$(ROOT_DIR)bin\</BIN_DIR>
    <INCLUDE_DIR>$(ROOT_DIR)include\</INCLUDE_DIR>
    <INTERMEDIATE_DIR>$(ROOT_DIR)intermediate\$(SolutionName)\$(ProjectName)\</INTERMEDIATE_DIR>
    <LIB_DIR>$(ROOT_DIR)lib\</LIB_DIR>
    <SYMBOLS_DIR>$(ROOT_DIR)symbols\</SYMBOLS_DIR>
    <CHROOT_DBG>D:\gtkmm\output\Debug\$(Platform)\</CHROOT_DBG>
    <CHROOT_REL>D:\gtkmm\output\Release\$(Platform)\</CHROOT_REL>
    <SET_INTERMEDIATE>intermediate\$(SolutionName)\$(ProjectName)\</SET_INTERMEDIATE>
  </PropertyGroup>
  <PropertyGroup>
    <LinkIncremental>false</LinkIncremental>
    <OutDir>$(ROOT_DIR)failed</OutDir>
    <IntDir>$(INTERMEDIATE_DIR)</IntDir>
    <GenerateManifest>false</GenerateManifest>
    <ExtensionsToDeleteOnClean>*.dll;*.ilk;*.pdb;*.exe;*.lib;$(ExtensionsToDeleteOnClean)</ExtensionsToDeleteOnClean>
  </PropertyGroup>
  <ItemDefinitionGroup>
    <ClCompile>
      <RuntimeLibrary>MultiThreadedDLL</RuntimeLibrary>
      <WarningLevel>Level3</WarningLevel>
      <Optimization>MaxSpeed</Optimization>
      <MinimalRebuild>false</MinimalRebuild>
      <CompileAsManaged>false</CompileAsManaged>
      <CompileAs>Default</CompileAs>
      <EnableParallelCodeGeneration>true</EnableParallelCodeGeneration>
      <PreprocessorDefinitions>WIN32;_CRT_SECURE_NO_WARNINGS;_SCL_SECURE_NO_WARNINGS;_CRT_NONSTDC_NO_DEPRECATE;NTDDI_WIN7=0x06010000;_WIN32_WINNT_WIN7=0x0601;%(PreprocessorDefinitions)</PreprocessorDefinitions>
      <FloatingPointExceptions>false</FloatingPointExceptions>
      <StringPooling>true</StringPooling>
      <SDLCheck>false</SDLCheck>
      <MultiProcessorCompilation>false</MultiProcessorCompilation>
      <DebugInformationFormat>ProgramDatabase</DebugInformationFormat>
      <RuntimeTypeInfo>true</RuntimeTypeInfo>
      <BrowseInformation>false</BrowseInformation>
      <FunctionLevelLinking>false</FunctionLevelLinking>
      <IntrinsicFunctions>true</IntrinsicFunctions>
      <WholeProgramOptimization>true</WholeProgramOptimization>
      <InlineFunctionExpansion>AnySuitable</InlineFunctionExpansion>
      <TreatSpecificWarningsAsErrors>4098</TreatSpecificWarningsAsErrors>
      <SuppressStartupBanner>false</SuppressStartupBanner>
    </ClCompile>
    <Link>
      <GenerateDebugInformation>true</GenerateDebugInformation>
      <SubSystem>NotSet</SubSystem>
      <RandomizedBaseAddress>false</RandomizedBaseAddress>
      <GenerateMapFile>false</GenerateMapFile>
      <MapFileName>$(OutDir)$(TargetName).map</MapFileName>
      <TreatLinkerWarningAsErrors>true</TreatLinkerWarningAsErrors>
      <ShowProgress>LinkVerboseSAFESEH</ShowProgress>
      <OptimizeReferences>true</OptimizeReferences>
      <MapExports>false</MapExports>
      <EnableCOMDATFolding>true</EnableCOMDATFolding>
      <GenerateWindowsMetadata>false</GenerateWindowsMetadata>
      <ImportLibrary>$(LIB_DIR)$(TargetName).dll.lib</ImportLibrary>
      <ImageHasSafeExceptionHandlers>false</ImageHasSafeExceptionHandlers>
      <ProgramDatabaseFile>$(SYMBOLS_DIR)$(TargetName).pdb</ProgramDatabaseFile>
      <LinkTimeCodeGeneration>UseLinkTimeCodeGeneration</LinkTimeCodeGeneration>
      <LinkStatus>false</LinkStatus>
      <AdditionalLibraryDirectories>$(LIB_DIR)</AdditionalLibraryDirectories>
      <AdditionalOptions>/machine:x64</AdditionalOptions>
      <SuppressStartupBanner>false</SuppressStartupBanner>
    </Link>
    <Lib>
      <TargetMachine>MachineX64</TargetMachine>
      <ErrorReporting>PromptImmediately</ErrorReporting>
      <TreatLibWarningAsErrors>true</TreatLibWarningAsErrors>
      <LinkTimeCodeGeneration>true</LinkTimeCodeGeneration>
    </Lib>
    <Midl>
      <WarningLevel>4</WarningLevel>
    </Midl>
    <ProjectReference>
      <LinkLibraryDependencies>false</LinkLibraryDependencies>
    </ProjectReference>
  </ItemDefinitionGroup>
  <ItemGroup>
    <BuildMacro Include="ROOT_DIR">
      <Value>$(ROOT_DIR)</Value>
    </BuildMacro>
    <BuildMacro Include="BIN_DIR">
      <Value>$(BIN_DIR)</Value>
    </BuildMacro>
    <BuildMacro Include="INCLUDE_DIR">
      <Value>$(INCLUDE_DIR)</Value>
    </BuildMacro>
    <BuildMacro Include="INTERMEDIATE_DIR">
      <Value>$(INTERMEDIATE_DIR)</Value>
    </BuildMacro>
    <BuildMacro Include="LIB_DIR">
      <Value>$(LIB_DIR)</Value>
    </BuildMacro>
    <BuildMacro Include="SYMBOLS_DIR">
      <Value>$(SYMBOLS_DIR)</Value>
    </BuildMacro>
    <BuildMacro Include="CHROOT_DBG">
      <Value>$(CHROOT_DBG)</Value>
    </BuildMacro>
    <BuildMacro Include="CHROOT_REL">
      <Value>$(CHROOT_REL)</Value>
    </BuildMacro>
    <BuildMacro Include="SET_INTERMEDIATE">
      <Value>$(SET_INTERMEDIATE)</Value>
    </BuildMacro>
  </ItemGroup>
</Project>